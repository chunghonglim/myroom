		<script type="text/javascript">
			(function(){
				var threeshoparea=document.getElementById('three-shoparea');
				var h2=threeshoparea.getElementsByTagName('h2')[0];
				var a=h2.getElementsByTagName('a');			/*切记最后一个a标签不要取到。只取0 1 2 */
				var hot=threeshoparea.getElementsByClassName('hot');
				var num;       /*索引序列值*/
				a[0].className='bortom';	/*初始化*/
				a[0].style.opacity='1';
				/*绑定a标签mouseover事件*/
				for(var j=0;j<a.length-1;j++){
					a[j].index=j;							/*自定义属性储存序列值*/
					num=a[j].index;
					a[j].onmouseover=function(){
						num=this.index;							/*获取当前事件的序列值*/
						for(var k=0;k<a.length-1;k++){				/*每次移动到其他地方后清除其他样式*/
							a[k].style.opacity='.4';
							a[k].className='';							
							hot[k].style.display='none';
						}						
						this.style.opacity='1';
						this.className='bortom';
						hot[num].style.display='block';
					}
				}
			})()
		</script>
